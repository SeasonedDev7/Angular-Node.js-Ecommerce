  "license": "MIT",
  "engines": {
    "node": "11.2.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "node dist/server.js",
    "ssr": "npm run build:ssr && npm run start",
    "build:ssr": "npm run build:client-and-server-bundles && npm run ssr:webpack",
    "build:client-and-server-bundles": "ng build --aot --prod --output-hashing none && ng run eshop:server",
    "ssr:webpack": "webpack --config webpack.server.config.js --progress --colors --mode development",
    "ssr:webpackprod": "webpack --config webpack.server.config.js --progress --colors --mode production",
    "lint": "ng lint",
    "heroku-postbuild": "npm run build:client-and-server-bundles && npm run ssr:webpackprod"
  },
